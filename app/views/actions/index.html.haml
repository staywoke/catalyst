.col-xs-12
  %h1 Actions

.col-xs-12
  %table.table.table-hover
    %thead
      %tr
        %th Name
        %th Type
        %th Locality
        %th State
    %tbody
      - @tasks.each do |task|
        %tr
          %td= task.project::NAME
          %td= task.type
          %td= task.locality.name
          %td= task.locality.state
